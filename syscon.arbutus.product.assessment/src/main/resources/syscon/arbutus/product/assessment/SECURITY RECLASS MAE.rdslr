package syscon.arbutus.product.assessment;

import syscon.arbutus.product.services.assessment.contract.dto.AssessedNeeds;

rule "SECURITY RECLASS MAE"
	dialect "mvel"
	when
		Assessment Type is RECLASS and Assessment Score is greater than or equal to 146 and Assessment Score is less than or equal to 228
	then
		Assessment Result is MAE
end
