package syscon.arbutus.product.assessment;

import syscon.arbutus.product.services.assessment.contract.dto.AssessedNeeds;

rule "SECURITY RECLASS ME"
	dialect "mvel"
	when
		Assessment Type is RECLASS and Assessment Score is greater than or equal to 20 and Assessment Score is less than or equal to 145
	then
		Assessment Result is ME
end
